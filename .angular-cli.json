{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "tech-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets/images",
        "favicon.ico",
        "ngsw-worker.js"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "serviceWorker": true,
      "styles": [
        "styles.css",
        "../node_modules/snazzy-info-window/dist/snazzy-info-window.css",
        "assets/css/demo.css",
        "assets/css/component.css",
        "assets/css/animation.css",
        
        "assets/css/owl.carousel.css",
        "assets/css/owl.theme.default.css",
        "assets/css/style.css",
        "../node_modules/ngx-toastr/toastr.css",
        "assets/fonts/fontawesome/css/font-awesome.min.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/blast-text/jquery.blast.min.js",
        "../src/assets/thirdpartyjs/modernizr-custom.js",
        "../src/assets/thirdpartyjs/classie.js",
        "../src/assets/thirdpartyjs/main.js",        
        "../src/assets/thirdpartyjs/bootstrap.min.js",        
        "../src/assets/thirdpartyjs/owl.carousel.js",        
        "../src/assets/thirdpartyjs/script.js",        
        "../src/assets/thirdpartyjs/scrollIt.js",        
        "../src/assets/thirdpartyjs/TweenMax.js",
        "../src/assets/thirdpartyjs/typed.js",
        "../src/assets/thirdpartyjs/wow.min.js",
        "../src/assets/thirdpartyjs/app.js"   
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {
    }
  }
}
